int search(vector<int> &vec, int k)
{
    int l = 0;
    int r = vec.size();
    int m = l + (r-l)/2;
    while(l < r)
    {
        if(k == vec[m]){return m;}
        if(k < vec[m]){r = m; m = l + (r-l)/2;}
        else{l = m; m = l + (r-l)/2;}
    }
    if(k == vec[m]){return m;}
    return -1;
}
