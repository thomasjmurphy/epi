string bigInt(string a, string b)
{
    string ret = "";
    string change = "";
    ret.resize(a.length()+b.length());
    change.resize(a.length()+b.length());
    for(int i = a.length()-1; i>=0; i--)
    {
        for(int j = b.length()-1; j>=0; j--)
        {
            ret[i+j+1] = (char)(((int)(a[i])-48)*((int)(b[j])-48)%10+48);
            change[i+j] = (char)(((int)(a[i])-48)*((int)(b[j])-48)/10+48);
        }
    }
    for(int i = 0;i<ret.length();i++)
    {
        if((int)(ret[i]-48) + (int)(change[i]-48) > 9)
        {
            
        }
    }
    return ret;
}

int main()
{
    string a = "123";
    string b = "456";
    cout << bigInt(a,b);
    return 0;
}
