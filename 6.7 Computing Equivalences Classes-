unordered_map<int,vector<int>> inverse(vector<int> vec)
{
    unordered_map<int,vector<int>> ret;
    for(int i =0 ;i<vec.size();i++)
    {
        ret[vec[i]].push_back(i);
    }
    return ret;
}

vector<vector<int>> equiv(vector<int> a, vector<int> b)
{
