struct node
{
    int val;
    node* next;
    node(int val) : val(val), next(NULL) {}
};

node* listOverlap(node* a, node* b)
{
    int aLength = 0;
    int bLength = 0;
    int BLow = 0;
    int bGuess = 0;
    int bHigh = 0;
    node* aTest = a;
    node* bTest = b;
    while(aTest != NULL)
    {
        aTest = aTest->next;
        aLength++;
    }
    while(bTest != NULL)
    {
        bTest = bTest->next;
        bLength++;
    }
    while(aTest != bTest)
    {
        
    }
    
    
    
}

int main()
{
    node* n1 = new node(1);
    node* n2 = new node(2);
    node* n3 = new node(3);
    node* n4 = new node(4);
    node* n5 = new node(5);
    node* n6 = new node(6);
    n1->next = n2;
    n2->next = n3;
    n3->next = n4;
    n4->next = n5;
    n5->next = n6;
